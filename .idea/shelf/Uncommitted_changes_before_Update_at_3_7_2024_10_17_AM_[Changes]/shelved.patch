+package practiceprograms;
+
+public class Stringuppercase_lowercase {
+
+    public static void main(String[] args) {
+
+        String name = "Rohit Abaso kamble";
+
+        // converting into the uppercase letter string
+        System.out.println("The uppercase letters of the string is :-" + name.toUpperCase());
+
+
+        // conversting string into the lowercase letters
+        System.out.println("The uppercase letters of the string is :-" + name.toLowerCase());
+    }
+}
